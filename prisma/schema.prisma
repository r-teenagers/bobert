generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./bobert.db"
}

model Team {
  snowflake String   @unique
  players    Player[]
}

model Player {
  snowflake     String  @unique
  team          Team    @relation(fields: [teamSnowflake], references: [snowflake])
	teamSnowflake String
  score         Int
  blacklisted   Boolean @default(false)
}
